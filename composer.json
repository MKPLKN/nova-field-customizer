{
    "name": "bhnow/nova-field-customizer",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bhnow\\NovaFieldCustomizer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bhnow\\NovaFieldCustomizer\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
